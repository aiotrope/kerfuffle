{
  "name": "backend",
  "version": "0.0.1",
  "private": true,
  "description": "Main backend of kerfuffle",
  "main": "./dist/index.js",
  "scripts": {
    "babel-node": "babel-node --presets='@babel/preset-env'",
    "dev": "NODE_ENV=development nodemon --exec yarn run babel-node -- src/index.js",
    "build:server": "rimraf dist/ && babel src --out-dir dist --copy-files",
    "start": "NODE_ENV=production node ./dist/index.js",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.+(js|jsx|json|md|ts|tsx|css|scss)\"",
    "linting": "eslint --ignore-path .eslintignore './**/*.{ts,mjs,js}'",
    "lint": "yarn run linting -- --fix",
    "start:testMode": "NODE_ENV=test nodemon --exec yarn run babel-node -- src/index.js",
    "test": "NODE_ENV=test jest --verbose --runInBand --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aiotrope/kerfuffle.git"
  },
  "author": "Arnel Imperial",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aiotrope/kerfuffle/issues"
  },
  "homepage": "https://github.com/aiotrope/kerfuffle#readme",
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72",
  "dependencies": {
    "@babel/register": "^7.23.7",
    "@babel/runtime": "^7.24.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "^4.18.3",
    "express-async-errors": "^3.1.1",
    "express-jwt": "^8.4.1",
    "http-errors": "^2.0.0",
    "ioredis": "^5.3.2",
    "isomorphic-dompurify": "^2.5.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nocache": "^4.0.0",
    "nodemon": "^3.1.0",
    "postgres": "^3.4.3",
    "rimraf": "^5.0.5",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.1",
    "@babel/node": "^7.23.9",
    "@babel/plugin-transform-class-properties": "^7.24.1",
    "@babel/plugin-transform-object-rest-spread": "^7.24.1",
    "@babel/plugin-transform-runtime": "^7.24.1",
    "@babel/preset-env": "^7.24.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4"
  }
}
